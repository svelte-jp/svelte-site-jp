{
  "name": "svelte.dev",
  "version": "1.0.0",
  "description": "Docs and examples for Svelte",
  "scripts": {
    "dev": "npm run copy-workers && sapper dev",
    "copy-workers": "node scripts/copy-workers.js",
    "migrate": "node-pg-migrate -r dotenv/config",
    "build": "npm run copy-workers && sapper build --legacy",
    "update": "node scripts/update_template.js && node scripts/get-contributors.js && node scripts/update_whos_using.js",
    "start": "node __sapper__/build",
    "lint": "node -e \"var shell=require('shelljs');var files=shell.find(['./CONTRIBUTING.md','./content/**/**/*.ja.md']).join(' ');if(shell.exec('textlint -f pretty-error '+files).code!==0){shell.exit(1)};\"",
    "test": "mocha -r esm test/**",
    "deploy": "make deploy"
  },
  "dependencies": {
    "@polka/redirect": "^1.0.0-next.7",
    "@polka/send": "^1.0.0-next.7",
    "cookie": "^0.4.0",
    "devalue": "^2.0.0",
    "do-not-zip": "^1.0.0",
    "flru": "^1.0.2",
    "httpie": "^1.1.2",
    "jsonwebtoken": "^8.5.1",
    "marked": "^1.0.0",
    "pg": "^7.12.1",
    "polka": "^1.0.0-next.9",
    "prism-svelte": "^0.4.3",
    "prismjs": "^1.21.0",
    "sirv": "^1.0.0",
    "svelte-i18n": "^3.3.6",
    "yootils": "0.0.16"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@sindresorhus/slugify": "^0.9.1",
    "@sveltejs/site-kit": "^1.2.5",
    "@sveltejs/svelte-repl": "^0.2.1",
    "all-contributors-cli": "^6.20.0",
    "degit": "^2.1.4",
    "dotenv": "^8.1.0",
    "esm": "^3.2.25",
    "jimp": "^0.8.0",
    "mocha": "^6.2.0",
    "node-fetch": "^2.6.1",
    "node-pg-migrate": "^3.22.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.30.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.10",
    "shelljs": "^0.8.3",
    "svelte": "^3.12.0",
    "textlint": "^11.9.0",
    "textlint-rule-detect-bad-chars": "^1.0.2",
    "textlint-rule-no-mix-dearu-desumasu": "^4.0.1",
    "textlint-rule-preset-jtf-style": "^2.3.8"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
